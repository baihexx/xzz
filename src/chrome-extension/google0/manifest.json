{
  "manifest_version": 2,
  "name": "google页面简化",
  "version": "0.1",
  "description": "google页面--logo",
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png"
    },
    "default_title": "google logo 隐藏",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
     "matches": ["*://www.google.com.tw/*", "*://www.google.com/*"],
     "js": ["js/content_script.js"]
  }],
  "permissions": [
    "tabs"
  ]
}
